begin
	integer x, stacksize, total;
	x := 0;
	total := 0;
	stacksize := 0;
	while x < 10 do begin
		push(x);
		printn(x);
		stacksize := stacksize + 1;
		x := x + 1
	end;
	print();
	x := 0;
	while x < stacksize do begin
		integer y;
		y :=  get(x);
		printn(y);
		total := total + y;
		x := x + 1
	end;
	while 0 # stacksize do begin
		pop();
		stacksize := stacksize - 1
	end;
	print();
	prints("TOTAL: ");
	printn(total);
	print()
end