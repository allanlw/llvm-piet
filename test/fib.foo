begin
	integer current, previous, i, n;
	n := 10;
	current := 1;
	previous := 1;
	i := 2;
	while i<n do begin integer temp;
		temp := current;
		current := current + previous;
		previous := temp;
		i := i + 1
	end;
	prints("The ");
	printn(n);
	prints("th Fibonacci number is :");
	printn(current);
	prints("\n")
end
